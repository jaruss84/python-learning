content = '''
5
15 

40
'''



lines = content.split('\n')
total = 0
count = 0
for line in lines:
    if line != '':
        expense = int(line)
        print(expense)
        total += expense
        count += 1
        
print('Liczba wydatków: ', count)
print('Suma wydatków', total)
average = total/count
print('Średnia wydatków', average)
